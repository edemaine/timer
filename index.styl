body
  font-family: 'Roboto', sans-serif
  margin: 0px
  display: flex
  flex-direction: column
svg
  margin: 10px
  flex-grow: 1
text
  text-anchor: middle
  font-size: 30px
.colon
  fill: #666
text.overtime
  font-size: 8px

.overtime
  display: none
.negative
  .overtime
    display: block
  text
    fill: Crimson
